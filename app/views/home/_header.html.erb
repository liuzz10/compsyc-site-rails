<div class="top-bar" id="example-menu">
  <div class="top-bar-left">
    <ul class="dropdown menu" data-dropdown-menu>
      <%= link_to 'Compsyc', root_path, class:"navbar-brand" %>
      <li>
        <button
          type="button"
          data-bs-toggle="collapse"
          data-bs-target="#navbarSupportedContent"
          aria-controls="navbarSupportedContent"
          aria-expanded="false"
          aria-label="Toggle navigation"
        >
          <span></span>
        </button>
      </li>
      <li class="nav-btn">
        <%= link_to 'Videos', videos_path, class:"nav-link" %>
      </li>
      <li class="nav-btn">
        <%= link_to 'Articles', articles_path, class:"nav-link" %>
      </li>
      <li class="nav-btn">
        <%= link_to 'About', home_about_path, class:"nav-link" %>
      </li>
    </ul>
  </div>
  <div class="top-bar-right">
    <%# registration %>
    <ul class="menu align-right">
      <% if user_signed_in? %>
      <li class="nav-btn">
        <%= link_to 'Sign out', destroy_user_session_path, data:
        {"turbo-method": :delete}, class:"nav-link"%>
      </li>
      <li class="nav-btn">
        <%= link_to 'Edit profile', edit_user_registration_path,
        class:"nav-link" %>
      </li>
      <% else %>
      <li class="nav-btn">
        <%= link_to 'Sign up', new_user_registration_path, class:"nav-link" %>
      </li>
      <li class="nav-btn">
        <%= link_to 'Sign in', new_user_session_path, class:"nav-link" %>
      </li>
      <% end %>
    </ul>
  </div>
</div>
